<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>User_date__c</fullName>
    <description>Convert StarDate into user friendly date</description>
    <externalId>false</externalId>
    <formula>TEXT(DAY(StartDate))+&quot;/&quot;+TEXT(MONTH(StartDate))+&quot;/&quot;+TEXT(YEAR(StartDate))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>User_date</label>
    <required>false</required>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
